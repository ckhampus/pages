{
    "name": "ckhampus/pages",
    "description": "Really simple CMS.",
    "authors": [
        {
            "name": "Cristian Hampus",
            "email": "contact@cristianhampus.se"
        }
    ],
    "require": {
        "php": ">=5.4",
        "silex/silex": "1.0.x-dev",
        "doctrine/orm": "2.3.x-dev",
        "doctrine/migrations": "*",
        "doctrine/data-fixtures": "*",
        "symfony/browser-kit": "2.1.x-dev",
        "symfony/console": "2.1.x-dev",
        "symfony/twig-bridge": "2.1.x-dev",
        "symfony/doctrine-bridge": "2.1.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.8",
        "fabpot/php-cs-fixer": "*"
    },
    "autoload": {
        "psr-0": { "Pages": "src/" }
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    }
}
